{
    "name": "justnotiyann/filament-schema-resource",
    "description": "Artisan command to generate Filament form & table schema files based on Eloquent model.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "artisan",
        "generator",
        "schema"
    ],
    "homepage": "https://github.com/justnotiyann/filament-schema-resource",
    "support": {
        "issues": "https://github.com/justnotiyann/filament-schema-resource/issues",
        "source": "https://github.com/justnotiyann/filament-schema-resource"
    },
    "authors": [
        {
            "name": "justnotiyann",
            "email": "muhammadfitrian0712@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Justnotiyann\\FilamentSchemaResource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Justnotiyann\\FilamentSchemaResource\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Justnotiyann\\FilamentSchemaResource\\FilamentSchemaResourceServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "filament/filament": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
